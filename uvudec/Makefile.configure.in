PACKAGE_NAME=@PACKAGE_NAME@
# Defined in Makefile.version.in
#PACKAGE_VERSION=@PACKAGE_VERSION@

# Runs faster
USING_STATIC=Y
USING_DYNAMIC=Y
EXE_BUILD_SUFFIX=Y
# Speed up dev
USING_UPX=N

PREFIX=@prefix@
INSTALL_DIR=@install_dir@

# Doesn't seem to help
USING_ROPE=N

USING_QT=Y
USING_PYTHON=Y
USING_PYTHON_API=Y
USING_PYTHON_EXEC=Y
PYTHON_INCLUDE_DIR=@python_include_dir@
# Unused (was experimental for uvdasm)
USING_LUA=N
# Unused (was experimental for uvdasm)
USING_SPIDERAPE=N
USING_LIB_JANSSON=Y
# To make testing nice
DEFAULT_DECOMPILE_FILE='"@default_decompile_file@"'
# For playing around, use the build dir config, not the installed
DEFAULT_CPU_DIR='"arch"'
DEFAULT_CPU_FILE=$(DEFAULT_CPU_DIR)'"/@uvdasm_default_cpu_file@"'

# LUA_DIR=/home/mcmaster/document/software/lua/lua-5.1.4
# SPIDERAPE_DIR=/home/mcmaster/document/build/spider_ape/SpiderApe-2007.09.26
BINUTILS_PREFIX=@binutils_prefix@
QT_PREFIX=@qt_prefix@
# Qt dependency
X11R6_LIB_DIR=@x11r6_lib_dir@

USING_LIBBFD=Y

USING_UVUDEC=@enable_uvudec@
USING_BINTOOBJ=@enable_bin2obj@
USING_OBJ2PAT=@enable_obj2pat@
USING_PAT2SIG=@enable_pat2sig@
USING_FLIRTUTIL=@enable_flirtutil@
USING_GUI=@enable_GUI@

# Enable dep gen
MAKEDEPEND=makedepend

# Work off of current directory
INSTALLED_FILES_RELATIVE=Y

